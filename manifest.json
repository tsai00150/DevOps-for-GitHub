{
  "name": "GitHub For DevOps",
  "description": "A Google Chrome extension that can visualize DevOps evaluation metrics.",
  "icons": {
    "16": "icon.png",
    "48": "icon.png",
    "128": "icon.png"
  },
  "action":{
      "default_popup": "index.html"
  },
  "content_scripts": [
    {
      "matches": ["https://github.com/*"],
      "js": ["script.js"]
    }
  ],
  "manifest_version": 3,
  "version": "1.0"
}